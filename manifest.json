{

    "manifest_version": 2,
    "name": "WorkGarden",
    "version": "1.0",
  
    "description": "Motywuje do nie korzystania ze stron zabierających czas, tylko do pracowania tworząc ogródek.",
  

    "background": {
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    
    "options_page": "options.html",
    "permissions": [
        "tabs"
    ]
        
    
    
    
  
  }
  